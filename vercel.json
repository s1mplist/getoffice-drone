{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "uv sync --no-dev --frozen",
    "framework": null,
    "outputDirectory": "api",
    "functions": {
        "api/**/*.py": {
            "memory": 1024,
            "maxDuration": 10,
            "excludeFiles": "{tests/**,__tests__/**,**/*.test.py,**/test_*.py,fixtures/**,__fixtures__/**,testdata/**,sample-data/**,static/**,assets/**}"
        }
    }
}